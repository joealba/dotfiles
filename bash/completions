$PREFIX = `brew --prefix`

if [ -f $PREFIX/etc/bash_completion ]; then
  . $PREFIX/etc/bash_completion
fi

if [ -f $PREFIX/lib/git/contrib/completion/git-completion.bash ]; then
	source $PREFIX/lib/git/contrib/completion/git-completion.bash
fi

